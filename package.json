{
  "name": "cryptounit-converter",
  "version": "1.0.2",
  "description": "easily convert btc, ether, bnb to their crypto unit",
  "main": "./src/index.js",
  "scripts": {
    "dev": "nodemon --exec babel-node src/index.js",
    "build": "babel src/index.js --out-file dist/index.js",
    "launch": "npm run build && npm run start",
    "start": "node build/index.js",
    "lint": "eslint src",
    "lint-fix": "eslint src --ext .js --fix",
    "test": "jest --verbose --runInBand"
  },
  "files": [
    "examples",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codaelux/cryptounit-converter.git"
  },
  "keywords": [
    "bitcoin",
    "bitcoin converter",
    "bitcoin satoshi converter",
    "cryptocurrency",
    "binance converter",
    "binance",
    "ether",
    "ethereum converter",
    "ether",
    "crypto converter",
    "conversion"
  ],
  "author": {
    "name": "Joseph Peculiar",
    "url": "https://t.me/@Joecryptograph"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codaelux/cryptounit-converter/issues"
  },
  "homepage": "https://github.com/codaelux/cryptounit-converter#readme",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "jest": "^25.2.3"
  },
  "dependencies": {
    "bignumber.js": "^9.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0"
  }
}
